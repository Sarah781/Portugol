programa {
  funcao inicio() {
    inteiro funcionario
    inteiro horas
    real salario
    real totalsalario

    escreva(funcionario)
    leia(funcionario)

    escreva(horas)
    leia(horas)

    escreva(salario)
    leia(salario)

    totalsalario=(salario*horas)

    escreva("NUMBER = ", funcionario, "\n", "SALARY = U$ ", totalsalario)
  }
}
